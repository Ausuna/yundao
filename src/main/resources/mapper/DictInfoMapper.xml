<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yundao.dao.DictInfoDao">
    <select id="listDict" resultType="com.yundao.bean.DictInfo">
        select * from dict_info
    </select>

    <select id="listDictDetail" resultType="com.yundao.bean.DictDetail">
        select * from dict_detail where dict_id = #{dictId}
    </select>

    <insert id="insertDictInfo" parameterType="com.yundao.bean.DictInfo">
        insert into dict_info values (#{id},#{dictId},#{dictDesc},#{createDate},#{createBy},#{modifyDate},#{modifyBy})
    </insert>
</mapper>